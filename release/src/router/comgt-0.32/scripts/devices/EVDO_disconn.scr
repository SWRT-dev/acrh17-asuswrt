opengt
set com 460800n81
set senddelay 0.05
waitquiet 1 0.2
send "ATH^m"
waitfor 6 "OK","ERR","ERROR"
if % = -1 goto timeerror
if % = 0 goto ok
if % = 1 goto error
if % = 2 goto error
:ok
print "DISCONNECTED\n"
exit 0
:error
print "DISCONNECT ERROR\n"
exit 1
:timeerror
print "DISCONNECT TIMEOUT\n"
exit 1

