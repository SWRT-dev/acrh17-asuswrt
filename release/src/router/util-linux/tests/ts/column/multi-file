TS_TOPDIR="$(dirname $0)/../.."
TS_DESC="multiple files"

. $TS_TOPDIR/functions.sh
ts_init "$*"

cd $TS_OUTDIR

$TS_CMD_COLUMN -x -c 50 $TS_SELF/input \
			$TS_SELF/input \
			$TS_SELF/input >> $TS_OUTPUT 2>&1

ts_finalize

