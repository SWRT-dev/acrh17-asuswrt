TS_TOPDIR="$(dirname $0)/../.."
TS_DESC="basic limits"

. $TS_TOPDIR/functions.sh
ts_init "$*"

. $TS_SELF/functions.sh

ts_log "check for difference between kernel and IPC"
ipcs_limits_check >> $TS_OUTPUT

ts_finalize

